@model IEnumerable<InvoiceERP.Models.TblUserType>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserType)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.UserTypeId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.UserTypeId">Details</a> |
                    <form asp-action="Delete" asp-route-id="@item.UserTypeId" method="post" onsubmit="return confirm('Are you sure you want to delete this item?');">
                        <input type="submit" value="Delete" class="btn btn-danger" />
                        @Html.AntiForgeryToken()
                    </form>
                </td>
            </tr>
        }
    </tbody>
</table>